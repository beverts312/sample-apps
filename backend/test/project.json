{
    "version": "1.0.0",
    "testRunner": "xunit",
    "dependencies": {
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "Moq": "4.6.25-alpha",
        "Microsoft.DotNet.InternalAbstractions": "1.0.0",
        "src": "1.0.0"
    },
    "frameworks": {
        "netcoreapp1.1": {
            "imports": [
                "dnxcore50"
            ],
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.1.0"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
